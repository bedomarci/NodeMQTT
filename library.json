{
  "name": "NodeMQTT",
  "keywords": "mqtt, iot, esp",
  "version": "0.5.0",
  "authors": {
    "name": "Marci Bedo",
    "email": "bedomarci@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bedomarci/NodeMQTT.git"
  },
  "license": "AGPL-3.0-or-later",
  "frameworks": "arduino",
  "platforms": [
    "espressif32",
    "espressif8266",
    "atmelavr"
  ],
  "build": {
    "libCompatMode": 2,
    "extraScript": "scripts/pio_set_flags.py"
  },
  "dependencies": [
    {
      "name": "SPI"
    },
    {
      "name": "khoih-prog/Functional-Vlpp"
    },
    {
      "name": "featherfly/SoftwareSerial",
      "platforms": "atmelavr"
    },
    {
      "name": "plerup/EspSoftwareSerial",
      "platforms": "espressif8266"
    },
    {
      "name": "TaskScheduler"
    },
    {
      "name": "DFRobotDFPlayerMini"
    },
    {
      "name": "LinkedList"
    },
    {
      "name": "PubSubClient"
    },
    {
      "name": "ArduinoJson"
    },
    {
      "name": "JC_Button"
    },
    {
      "name": "ccronexpr",
      "platforms": [
        "espressif8266",
        "espressif32"
      ],
      "repository": {
        "type": "git",
        "url": "https://github.com/staticlibs/ccronexpr"
      }
    },
    {
      "name": "RF24",
      "platforms": "atmelavr",
      "repository": {
        "type": "git",
        "url": "https://github.com/nRF24/RF24.git"
      }
    },
    {
      "name": "RF24Network",
      "platforms": "atmelavr",
      "repository": {
        "type": "git",
        "url": "https://github.com/nRF24/RF24Network.git"
      }
    },
    {
      "name": "RF24Mesh",
      "platforms": "atmelavr",
      "repository": {
        "type": "git",
        "url": "https://github.com/nRF24/RF24Mesh.git"
      }
    },
    {
      "name": "ESP8266WiFi",
      "version": "*",
      "platforms": "espressif8266"
    },
    {
      "name": "ESP8266HTTPClient",
      "version": "*",
      "platforms": "espressif8266"
    },
    {
      "name": "ESP8266httpUpdate",
      "version": "*",
      "platforms": "espressif8266"
    }
  ]
}